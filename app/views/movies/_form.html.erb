<%= render "layouts/errors", :object => @movie %>

<%= form_for @movie do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :genre %>
  <%= f.text_field :genre %>
  <%= f.label :rating %>
  <%= f.text_field :rating %>
  <%= f.label :synopsis %>
  <%= f.text_field :synopsis %>
  <%= f.submit %>
<% end %>
